@InProceedings{Peersman2012CLC,
  author           = {Claudia Peersman and Frederik Vaassen and Vincent Van Asch and Walter Daelemans},
  booktitle        = {CLEF},
  title            = {Conversation Level Constraints on Pedophile Detection in Chat Rooms},
  year             = {2012},
  abstract         = {In this paper we present a new approach for detecting online pedophiles in chat rooms that combines the results of predictions on the level of the individual post, the level of the user and the level of the entire conversation, and describe the results of this three-stage system in the PAN 2012 competition. Also, we describe a resampling and a filtering strategy to circumvent issues regarding the unbalanced dataset. Finally, we describe the creation of a dictionary of words and expressions relating to predators’ grooming stages,
which we used to identify which posts in the predators’ conversations were most distinctive for their grooming behavior.},
  file             = {:C\:/Users/marce/Documents/Meus Documentos/Vida Acadêmica/UFRJ/Mestrado/Dissertação/Revisão Bibliográfica/Artigos/Essenciais/Conversation Level Constraints on Pedophile Detection in Chat Rooms.pdf:PDF},
